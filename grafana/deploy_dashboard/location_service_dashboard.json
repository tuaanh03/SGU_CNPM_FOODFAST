      "title": "Location Service - Overall",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "id": 1,
      "targets": [ { "expr": "sum(rate(location_service_http_requests_total[5m]))", "refId": "A" } ],
      "title": "Request Rate (RPS)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "id": 2,
      "targets": [
        { "expr": "100 * sum(rate(location_service_http_requests_total{status_code=~\"4..\"}[5m])) / sum(rate(location_service_http_requests_total[5m]))", "legendFormat": "4xx", "refId": "A" },
        { "expr": "100 * sum(rate(location_service_http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(location_service_http_requests_total[5m]))", "legendFormat": "5xx", "refId": "B" }
      ],
      "title": "Error Rate",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "percent" } }
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "id": 3,
      "targets": [
        { "expr": "histogram_quantile(0.95, sum by (le) (rate(location_service_http_request_duration_seconds_bucket[5m]))) * 1000", "legendFormat": "p95", "refId": "A" }
      ],
      "title": "Response Time (p95)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "ms" } }
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "id": 4,
      "targets": [ { "expr": "100 * sum(rate(location_service_cache_operations_total{operation=\"hit\"}[5m])) / sum(rate(location_service_cache_operations_total{operation=~\"hit|miss\"}[5m]))", "refId": "A" } ],
      "title": "Cache Hit Rate (%)",
      "type": "stat",
      "fieldConfig": { "defaults": { "unit": "percent", "color": { "mode": "thresholds" }, "thresholds": { "steps": [ { "color": "red", "value": null }, { "color": "yellow", "value": 60 }, { "color": "green", "value": 80 } ] } } }
    },
