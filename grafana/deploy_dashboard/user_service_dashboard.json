{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": 0,
  "links": [],
  "panels": [
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "User Service - Overall",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "id": 1,
      "targets": [ { "expr": "sum(rate(user_service_http_requests_total[5m]))", "refId": "A" } ],
      "title": "Request Rate (RPS)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "id": 2,
      "targets": [
        { "expr": "100 * sum(rate(user_service_http_requests_total{status_code=~\"4..\"}[5m])) / sum(rate(user_service_http_requests_total[5m]))", "legendFormat": "4xx", "refId": "A" },
        { "expr": "100 * sum(rate(user_service_http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(user_service_http_requests_total[5m]))", "legendFormat": "5xx", "refId": "B" }
      ],
      "title": "Error Rate (4xx/5xx)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "percent" } }
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "id": 3,
      "targets": [
        { "expr": "histogram_quantile(0.50, sum by (le) (rate(user_service_http_request_duration_seconds_bucket[5m]))) * 1000", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum by (le) (rate(user_service_http_request_duration_seconds_bucket[5m]))) * 1000", "legendFormat": "p95", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum by (le) (rate(user_service_http_request_duration_seconds_bucket[5m]))) * 1000", "legendFormat": "p99", "refId": "C" }
      ],
      "title": "Response Time (p50/p95/p99 ms)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "ms" } }
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 9 },
      "id": 4,
      "targets": [ { "expr": "user_service_active_sessions_gauge", "refId": "A" } ],
      "title": "Active Sessions",
      "type": "gauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 17 },
      "id": 5,
      "targets": [ { "expr": "sum(rate(user_service_login_attempts_total[5m])) by (status)", "refId": "A" } ],
      "title": "Login Attempts (by status)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 17 },
      "id": 6,
      "targets": [
        {
          "expr": "sum(rate(user_service_registrations_total{role=~\"${role}\"}[5m])) by (role)",
          "legendFormat": "{{role}}",
          "refId": "A"
        }
      ],
      "title": "Registrations Rate (by role)",
      "type": "timeseries",
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0,
            "fillOpacity": 80,
            "gradientMode": "none",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "normal"
            }
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["lastNotNull", "sum"]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 4, "w": 12, "x": 0, "y": 25 },
      "id": 10,
      "targets": [
        {
          "expr": "sum(user_service_registrations_total{role=~\"${role}\"}) by (role)",
          "legendFormat": "{{role}}",
          "refId": "A",
          "instant": false
        }
      ],
      "title": "Total Registrations Count (Cumulative)",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 1000 }
            ]
          },
          "color": {
            "mode": "thresholds"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 25 },
      "id": 7,
      "targets": [ { "expr": "sum(rate(user_service_token_verifications_total[5m])) by (status)", "refId": "A" } ],
      "title": "Token Verifications",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 29 },
      "id": 8,
      "targets": [ { "expr": "user_service_process_resident_memory_bytes", "refId": "A" } ],
      "title": "Memory Usage",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "bytes" } }
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 29 },
      "id": 11,
      "targets": [ { "expr": "rate(user_service_process_cpu_seconds_total[5m]) * 100", "refId": "A" } ],
      "title": "CPU Usage (%)",
      "type": "timeseries",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "datasource": { "type": "prometheus", "uid": "grafana_prometheus" },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 35 },
      "id": 9,
      "targets": [ { "expr": "sum(rate(user_service_http_requests_total[5m])) by (method, route, status_code)", "refId": "A" } ],
      "title": "Request Count (by method/route/status)",
      "type": "table"
    }
  ],
  "schemaVersion": 42,
  "tags": [ "user-service", "monitoring" ],
  "templating": {
    "list": [
      {
        "current": { "text": "user_service", "value": "user_service" },
        "label": "service",
        "name": "service",
        "query": "metrics(.*_http_requests_total)",
        "regex": "^(.+)_http_requests_total$",
        "type": "query",
        "multi": false
      },
      {
        "current": { "text": "All", "value": "$__all" },
        "label": "HTTP Status",
        "name": "status",
        "query": "label_values(user_service_http_requests_total, status_code)",
        "type": "query",
        "includeAll": true,
        "allValue": "$__all",
        "multi": true
      },
      {
        "current": { "text": "All", "value": "$__all" },
        "label": "Role",
        "name": "role",
        "query": "label_values(user_service_registrations_total, role)",
        "type": "query",
        "includeAll": true,
        "allValue": "CUSTOMER|STORE_ADMIN|SYSTEM_ADMIN",
        "multi": true
      }
    ]
  },
  "time": { "from": "now-15m", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "User Service Dashboard",
  "uid": "user_service_dashboard",
  "version": 1
}

