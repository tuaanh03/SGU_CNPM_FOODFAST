        "type": "query",
        "multi": false
      },
      {
        "current": { "text": "All", "value": "$__all" },
        "label": "HTTP Status",
        "name": "status",
        "query": "label_values(user_service_http_requests_total, status_code)",
        "type": "query",
        "includeAll": true,
        "allValue": "$__all",
        "multi": true
      },
      {
        "current": { "text": "All", "value": "$__all" },
        "label": "Role",
        "name": "role",
        "query": "label_values(user_service_registrations_total, role)",
        "type": "query",
        "includeAll": true,
        "allValue": ".*",
        "multi": true
      }
    ]
  },
  "time": { "from": "now-15m", "to": "now" },
